<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <changeSet author="app" id="1.0">

        <createTable tableName="VET">
            <column name="ID" type="uuid">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="NAME" type="varchar(255)"/>
        </createTable>

        <createTable tableName="ELT_MAIN_ENTITY">
            <column name="ID" type="uuid">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="VERSION" type="int">
                <constraints nullable="false"/>
            </column>
            <column name="CREATE_TS" type="datetime"/>
            <column name="CREATED_BY" type="varchar(50)"/>
            <column name="UPDATE_TS" type="datetime"/>
            <column name="UPDATED_BY" type="varchar(50)"/>
            <column name="DELETE_TS" type="datetime"/>
            <column name="DELETED_BY" type="varchar(50)"/>
            <column name="NAME" type="varchar(255)"/>
        </createTable>

        <createTable tableName="ELT_MANY_ENTITY">
            <column name="ID" type="uuid">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="VERSION" type="int">
                <constraints nullable="false"/>
            </column>
            <column name="CREATE_TS" type="datetime"/>
            <column name="CREATED_BY" type="varchar(50)"/>
            <column name="UPDATE_TS" type="datetime"/>
            <column name="UPDATED_BY" type="varchar(50)"/>
            <column name="DELETE_TS" type="datetime"/>
            <column name="DELETED_BY" type="varchar(50)"/>
            <column name="NAME" type="varchar(255)"/>
            <column name="MAIN_ENTITY_ID" type="uuid">
                <constraints foreignKeyName="ELT_MAIN_ENTITY" references="ELT_MAIN_ENTITY(ID)"/>
            </column>
        </createTable>


        <createTable tableName="FP_MAIN_ENTITY">
            <column name="ID" type="uuid">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="VERSION" type="int">
                <constraints nullable="false"/>
            </column>
            <column name="CREATE_TS" type="datetime"/>
            <column name="CREATED_BY" type="varchar(50)"/>
            <column name="UPDATE_TS" type="datetime"/>
            <column name="UPDATED_BY" type="varchar(50)"/>
            <column name="DELETE_TS" type="datetime"/>
            <column name="DELETED_BY" type="varchar(50)"/>
            <column name="NAME" type="varchar(255)"/>
        </createTable>

        <createTable tableName="FP_MANY_ENTITY">
            <column name="ID" type="uuid">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="VERSION" type="int">
                <constraints nullable="false"/>
            </column>
            <column name="CREATE_TS" type="datetime"/>
            <column name="CREATED_BY" type="varchar(50)"/>
            <column name="UPDATE_TS" type="datetime"/>
            <column name="UPDATED_BY" type="varchar(50)"/>
            <column name="DELETE_TS" type="datetime"/>
            <column name="DELETED_BY" type="varchar(50)"/>
            <column name="NAME" type="varchar(255)"/>
            <column name="SECOND_NAME" type="varchar(255)"/>
            <column name="MAIN_ENTITY_ID" type="uuid">
                <constraints foreignKeyName="FP_MAIN_ENTITY" references="FP_MAIN_ENTITY(ID)"/>
            </column>
            <column name="ELT_MAIN_ENTITY_ID" type="uuid">
                <constraints foreignKeyName="ELT_MAIN_ENTITY" references="ELT_MAIN_ENTITY(ID)"/>
            </column>
        </createTable>


        <!-- ManyToMany -->
        <createTable tableName="TEST_MANY_TO_MANY_ENTITY">
            <column name="ID" type="uuid">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="VERSION" type="int">
                <constraints nullable="false"/>
            </column>
            <column name="CREATE_TS" type="datetime"/>
            <column name="CREATED_BY" type="varchar(50)"/>
            <column name="UPDATE_TS" type="datetime"/>
            <column name="UPDATED_BY" type="varchar(50)"/>
            <column name="DELETE_TS" type="datetime"/>
            <column name="DELETED_BY" type="varchar(50)"/>
            <column name="NAME" type="varchar(255)"/>
            <column name="SECOND_NAME" type="varchar(255)"/>
            <column name="MAIN_ENTITY_ID" type="uuid">
                <constraints foreignKeyName="FP_MAIN_ENTITY" references="FP_MAIN_ENTITY(ID)"/>
            </column>
        </createTable>

        <createTable tableName="TEST_MANY_TO_MANY_ENTITY_MANY_TO_MANY_TWO_ENTITY_LINK">
            <column name="MANY_TO_MANY_TWO_ENTITY_ID" type="uuid">
                <constraints
                        nullable="false" primaryKey="true"
                        primaryKeyName="PK_TEST_MANY_TO_MANY_ENTITY_MANY_TO_MANY_TWO_ENTITY_LINK"
                        foreignKeyName="MANY_TO_MANY_TWO_ENTITY_ID" references="MANY_TO_MANY_TWO_ENTITY_ID(ID)"
                />
            </column>
            <column name="MANY_TO_MANY_ENTITY_ID" type="uuid">
                <constraints
                        nullable="false" primaryKey="true"
                        primaryKeyName="PK_TEST_MANY_TO_MANY_ENTITY_MANY_TO_MANY_TWO_ENTITY_LINK"
                        foreignKeyName="MANY_TO_MANY_ENTITY_ID" references="MANY_TO_MANY_ENTITY_ID(ID)"
                />
            </column>
        </createTable>

        <createTable tableName="TEST_MANY_TO_MANY_TWO_ENTITY">
            <column name="ID" type="uuid">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="VERSION" type="int">
                <constraints nullable="false"/>
            </column>
            <column name="CREATE_TS" type="datetime"/>
            <column name="CREATED_BY" type="varchar(50)"/>
            <column name="UPDATE_TS" type="datetime"/>
            <column name="UPDATED_BY" type="varchar(50)"/>
            <column name="DELETE_TS" type="datetime"/>
            <column name="DELETED_BY" type="varchar(50)"/>
            <column name="NAME" type="varchar(255)"/>
            <column name="SECOND_NAME" type="varchar(255)"/>
        </createTable>

    </changeSet>

</databaseChangeLog>
